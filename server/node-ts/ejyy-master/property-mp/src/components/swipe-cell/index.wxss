@import '../common/index.wxss';

.cw-swipe-cell {
    position: relative;
    overflow: hidden;
}

.cw-swipe-cell__left,
.cw-swipe-cell__right {
    position: absolute;
    top: 0;
    height: 100%;
}

.cw-swipe-cell__left {
    left: 0;
    -webkit-transform: translate3d(-100%, 0, 0);
    transform: translate3d(-100%, 0, 0);
}

.cw-swipe-cell__right {
    right: 0;
    -webkit-transform: translate3d(100%, 0, 0);
    transform: translate3d(100%, 0, 0);
}
