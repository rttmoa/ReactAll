<view class="catelist-contanier"><swiper class="swiper" indicator-dots="{{true}}" autoplay="{{true}}" interval="{{3000}}" duration="{{1000}}"><block wx:for="{{cateObj.category.bannerUrlList}}" wx:for-item="item" wx:for-index="index" wx:key="index"><swiper-item><image src="{{item}}" mode></image></swiper-item></block></swiper><view class="title">{{''+cateObj.category.frontName+''}}</view><view class="desc">{{''+cateObj.category.frontDesc+''}}</view><view class="goods-list"><block wx:for="{{cateObj.itemList}}" wx:for-item="item" wx:for-index="__i0__" wx:key="id"><view data-event-opts="{{[['tap',[['toDetail',['$0'],[[['cateObj.itemList','id',item.id]]]]]]]}}" class="goods-item" bindtap="__e"><image class="goods-img" src="{{item.listPicUrl}}" mode></image><view class="goods-desc">{{''+item.name+''}}</view><view class="goods-price">{{''+item.retailPrice+''}}</view></view></block></view></view>