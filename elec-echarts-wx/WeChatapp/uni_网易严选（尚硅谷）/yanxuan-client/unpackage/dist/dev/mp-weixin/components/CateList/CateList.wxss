.catelist-contanier .swiper {
  height: 360rpx;
}
.catelist-contanier .swiper image {
  width: 100%;
  height: 360rpx;
}
.catelist-contanier .title {
  font-size: 40rpx;
  color: #333;
  text-align: center;
  height: 80rpx;
  line-height: 80rpx;
}
.catelist-contanier .desc {
  font-size: 30rpx;
  color: #666;
  text-align: center;
  height: 50rpx;
  line-height: 50rpx;
}
.catelist-contanier .goods-list {
  padding: 20rpx 0;
  display: flex;
  flex-wrap: wrap;
  justify-content: space-around;
}
.catelist-contanier .goods-list::after {
  width: 344rpx;
  content: '';
}
.catelist-contanier .goods-list .goods-item {
  width: 344rpx;
  margin-bottom: 20rpx;
}
.catelist-contanier .goods-list .goods-item .goods-img {
  width: 100%;
  height: 344rpx;
}
.catelist-contanier .goods-list .goods-item .goods-desc {
  font-size: 32rpx;
  color: #666;
}
.catelist-contanier .goods-list .goods-item .goods-price {
  color: #f00;
  font-size: 36rpx;
}

